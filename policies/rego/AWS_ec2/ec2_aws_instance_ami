package stackguardian.EC2.aws_instance_ami

#Ensure the ami of the aws_instance type will match with the given data by the user 

import input as tfplan

ami={
   resource_changes := tfplan.resource_changes[_]
   resource_changes.type == "aws_instance"
   resource_changes.change.after.ami == data.ami[_]
  
   
      
}